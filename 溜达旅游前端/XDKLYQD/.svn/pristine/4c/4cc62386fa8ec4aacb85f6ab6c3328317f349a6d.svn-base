
.head {
	margin-top: 8rpx;
	height: 96rpx;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	box-sizing: border-box;
	padding: 0 28rpx;
	background-color: #fff;
}
.head input {
	width: 90%;
}
.head image {
	width: 40rpx;
	height: 40rpx;
}
.uname {
	font-size: 34rpx;
	color: #999;
}
.success {
	margin: 44rpx auto;
	width: 690rpx;
	height: 85rpx;
	line-height: 85rpx;
	text-align: center;
	color: #fff;
	background-color: #59b9a6;
}

