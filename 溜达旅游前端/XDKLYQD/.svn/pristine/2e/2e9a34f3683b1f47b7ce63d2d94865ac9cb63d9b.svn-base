<view><view class="head"><input placeholder="请输入旧的密码" placeholder-class="password" data-event-opts="{{[['input',[['__set_model',['','usedPassword','$event',[]]]]]]}}" value="{{usedPassword}}" bindinput="__e"/><block wx:if="{{isUsedInput}}"><image src="/static/images/quxiao.png" data-event-opts="{{[['tap',[['usedRemove',['$event']]]]]}}" bindtap="__e"></image></block></view><view class="head"><input placeholder="请输入新的密码" placeholder-class="password" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><block wx:if="{{isInput}}"><image src="/static/images/quxiao.png" data-event-opts="{{[['tap',[['remove',['$event']]]]]}}" bindtap="__e"></image></block></view><view data-event-opts="{{[['tap',[['complete',['$event']]]]]}}" class="success" bindtap="__e">完成</view></view>